@model IEnumerable<EZYSoft2.Models.AuditLog>

<h2>Audit Logs</h2>
<table class="table">
    <thead>
        <tr>
            <th>Timestamp</th>
            <th>User ID</th>
            <th>Action</th>
            <th>IP Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
            <tr>
                <td>@log.Timestamp</td>
                <td>@log.UserId</td>
                <td>@log.Action</td>
                <td>@log.IPAddress</td>
            </tr>
        }
    </tbody>
</table>
